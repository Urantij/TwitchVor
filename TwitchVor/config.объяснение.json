{
    // Если указано, будет добавлять к стоимости стрима за каждый час.
    "Money": {
        "Currency": "RUB", // Допустимые значения RUB, USD
        "PerHourCost": 0.26
    },
    // Название канала
    "Channel": "Urantij",
    // Айди зарегистрированного приложения
    "TwitchAPIClientId": "",
    // Секрет зарегистрированного приложения
    "TwitchAPISecret": "",
    // Желаемое разрешение
    // По умолчанию 1280x720
    // Необязательно
    "PreferedVideoResolution": "1920x1080",
    // Желаемый фпс
    // По умолчанию 60
    // Необязательно
    "PreferedVideoFps": 60, //60 или 30
    // Если true, будет выбивать ошибку, если нет желаемого качества и фпс
    // Необязательно
    "TakeOnlyPrefered": false,

    // Если указано, будет создавать хранилище в виде S3 ведра от Timeweb
    "Timeweb": {
        "RefreshToken": "",
        "AccessToken": "",
        "AccessTokenExpirationDate": "30/12/2022 12:00:00 +00:00", // Именно в таком формате
        // Проверять токен при запуске вора
        "ValidateTokenOnStart": true
    },

    // Если указано, будет загружать видео в вк
    "Vk": {
        // Для загрузки видео
        // Нужен доступ video, offline
        "Uploader": {
            "ApiToken": "",
            "ApplicationId": 0
        },

        // Если указано, будет выкладывать пост с видео на стену группы
        // Нужны права wall, offline
        "WallRunner": {
            "ApiToken": "",
            "ApplicationId": 0
        },

        // Айди группы, куда выкладывать видео
        "GroupId": 123
    },
    
    // Если указано, будет конвертировать стрим во время загрузки
    "Conversion": {
        "FfmpegPath": "./ffmpeg/ffmpeg"
    },
    // Если указано, будет слать письма счастья
    "Email": {
        "Name": "Urantij",
        // Письма отправляются с этого емейла на этот же емейл
        "Email": "urantij@gmail.com",
        "Password": "", // Пароль нужен не от аккаунта, а сгенерированный для приложения
        // Если не удалось закончить загрузку
        "NotifyOnCriticalError": true,
        // Если всё загрузилось нормально
        "NotifyOnFinishSuccess": true
    },
    // Обязательно
    "Downloader": {
        // Не знаю, зачем я вынес клиент айди, он всегда один, и он ставится сам.
        "ClientId": null,
        // Если загрузка идёт с аккаунта, тут нужно указать его токен
        "OAuth": "OAuth ...",
        // Обязательно
        "UserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/106.0.0.0 Safari/537.36",
        // Если указано, будет проверять, кто подарил сабку на канале
        "SubCheck": {
            "AppClientId": "",
            "AppSecret": "",
            // Айди бота на твиче
            "UserId": "",
            // Нужно с аккаунта бота сделать авторизацию в приложении.
            "RefreshToken": "",
            // Будет проверять сабку при запуске приложения
            "CheckSubOnStart": true
        }
    },
    // Токены для просмотра стрима имеют срок жизни в 20 минут, но работают 30. 
    // Можно не ждать, пока они протухнут.
    "DownloaderForceTokenChange": false,
    // Как долго ещё пытаться загружать стрим, после его выключения
    "StreamContinuationCheckTime": "00:00:22",
    // Столько будет ждать вор, прежде чем начать загрузку стрима.
    // Если стрим переподрубится, будет считаться, как продолжение.
    "StreamRestartCheckTime": "01:00:00"
}